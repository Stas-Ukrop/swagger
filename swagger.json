{
    "openapi": "3.0.0",
    "info": {
        "title": "Greens Shop server",
        "description": "This is a simple server for shop.",
        "contact": {
            "name": "API Support",
            "email": "stkr.stkr342@gmail.com"
        },
        "license": {
            "name": "NodeJS 18."
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://greensserver.onrender.com/api",
            "description": "Development server"
        }
    ],
    "paths": {
        "/users/register": {
            "post": {
                "tags": [
                    "User"
                ],
                "description": "registration of user",
                "operationId": "register",
                "parameters": [
                    {
                        "in": "query",
                        "name": "name",
                        "description": "name of user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "email",
                        "description": "email of user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "role",
                        "description": "admin || user",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "password",
                        "description": "password of user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK"
                    },
                    "409": {
                        "description": "CONFLICT",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "message": "Email is already used"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/login": {
            "post": {
                "tags": [
                    "User"
                ],
                "description": "login",
                "operationId": "login",
                "parameters": [
                    {
                        "in": "query",
                        "name": "email",
                        "description": "email of user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "in": "query",
                        "name": "password",
                        "description": "password of user",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjYzZWU1MjUzZGViM2RjZDY2NDY2NzljZCIsImlhdCI6MTY3ODQ2NzY1NSwiZXhwIjoxNjc4NDc0ODU1fQ.LC5BlNE4wYJuZoAOb6TpF0cci9f8hwWOOg1FF-XX5Vg"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "UNAUTHORIZED",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "message": "Invalid credentials"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/logout": {
            "get": {
                "tags": [
                    "User"
                ],
                "description": "logout",
                "operationId": "logout",
                "parameters": [
                    {
                        "in": "header",
                        "name": "token",
                        "required": true,
                        "schema": {
                            "$ref": "#/components/schemas/Token"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "OK"
                    },
                    "401": {
                        "description": "UNAUTHORIZED",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "message": "Invalid credentials"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/getAll": {
            "get": {
                "tags": [
                    "User"
                ],
                "description": "getAll",
                "operationId": "getAll",
                "parameters": [
                    {
                        "in": "header",
                        "name": "token",
                        "required": true,
                        "schema": {
                            "$ref": "#/components/schemas/Token"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "message": "Invalid credentials"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "UNAUTHORIZED",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "type": "string",
                                        "example": {
                                            "message": "Invalid credentials"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "Token": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string",
                        "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6IjYzZWU1MjUzZGViM2RjZDY2NDY2NzljZCIsImlhdCI6MTY3ODQ2NzY1NSwiZXhwIjoxNjc4NDc0ODU1fQ.LC5BlNE4wYJuZoAOb6TpF0cci9f8hwWOOg1FF-XX5Vg"
                    }
                }
            }
        }
    }
}